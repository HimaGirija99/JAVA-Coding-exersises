import java.util.Scanner;

public class Coding {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int i, j, k;
        int N = sc.nextInt();
        int M =sc.nextInt();
        i = 1 ;
        while(i<=N)
        {
            j = 1 ;
            while( j<=M)
            {
                System.out.print("*");
                j++;
            }
            System.out.println();
            i++;
        }
    }
}
