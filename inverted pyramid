import java.util.Scanner;

public class patterns {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        System.out.println();
        while(n >= 1)
        {   int j=1;
            while(j<=n)
            {
                System.out.print("*");
                j++;
            }
            System.out.println();
            n--;
        }
    }
}

